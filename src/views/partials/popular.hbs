<div class="card mb-2">
    <div class="card-header bg-primary text-white">
        <h5> <i class="fas fa-fire"></i> Most Popular Images</h5>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each sidebar.popular}}
                <div class="col-md-6">
                    <a href="/images/{{uniqueId}}">
                        <img src="/public/upload/{{filename}}" class="popImg w-100 img-thumbnail">
                    </a>
                </div>
            {{/each}}
        </div>
    </div>
</div>